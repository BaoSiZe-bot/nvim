        "body": [
            "std::vector<int> prm;$1",
            "inline void oba(const int &lim)",
            "{",
            "    bool G[lim + 5];",
            "    for (int i = 2; i <= lim; ++i)",
            "    {",
            "        if (!G[i])",
            "            prm.push_back(i);",
            "        for (int j : prm)",
