    "spfa": {
        "prefix": "spfa",
        "body": [
            "inline void spfa(int s)",
            "{",
            "    for (int i = 1; i <= n; ++i)",
            "        d[i] = INT_MAX;",
            "    queue<int> q;",
            "    q.push(s);",
            "    d[s] = 0;",
            "    f[s] = 1;",
            "    while(!q.empty())",
            "    {",
            "        int now = q.front();",
            "        q.pop();",
            "        f[now] = 0;",
            "        for (int i = ed[now]; i; i = e[i].p) {",
            "            int to = e[i].v;",
            "            if(d[now] + e[i].w < d[to])",
            "            {",
            "                d[to] = d[now] + e[i].w;",
            "                if(!f[to])",
            "                {",
            "                    q.push(to);",
            "                    f[to] = 1;",
            "                }",
            "            }",
            "        }",
            "    }",
            "}"
        ]
    },
