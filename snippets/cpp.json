    "Quick pow(mod p)": {
        "prefix": [
            "qpmod",
            "ksmmod"
        ],
        "body": [
            "inline int qp(int a, int b, int p)",
            "{",
            "    int res = 1;",
            "    while(b)",
            "    {",
            "        if (b & 1)",
            "            res = res * a % p;",
            "        a = a * a % p;",
            "        b >>= 1;",
            "    }",
            "    return res;",
            "}"
